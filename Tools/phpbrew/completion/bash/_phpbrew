  case $argument_index in
      0)
      COMPREPLY=( $(compgen -W "root
home
build
bin
include
etc
ext
ext-src
extension-src
extension-dir
config-scan
dist" -- $cur) )
      return
      ;;
  esac
  fi

                # If there is no argument support, then user is supposed to give a subcommand name or an option
